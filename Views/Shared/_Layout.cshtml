<!DOCTYPE html>

<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>@ViewData["Title"]</title>
	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css" type="text/css">
	<link rel="stylesheet" href="~/bootstrap/css/bootstrap.min.css" type="text/css" />
	<link rel="stylesheet" href="~/StyleSheet.css" type="text/css" />
</head>
<body class="bg-body">
    <header class="mb-3">@await Html.PartialAsync("_PartialHeader")</header>
    @RenderSection("PageBanner", false)
	<main class="min-vh-100 container mb-3">@RenderBody()</main>
	<footer class="bg-dark">@await Html.PartialAsync("_PartialFooter")</footer>
    <script src="~/bootstrap/js/bootstrap.bundle.min.js" crossorigin="anonymous" async></script>
	<script src="~/JavaScriptModules.js" type="module" async></script>
    <script src="~/JavaScript.js" type="module" async></script>
</body>
</html>